<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="productos.Categoria" table="Categoria" lazy="false">
        <id name="id" column="id" type="integer" unsaved-value="0">
            <generator class="native">
            </generator>
        </id>
        <property name="nombreCategoria" column="nombreCategoria" type="integer" length="10" not-null="false"
                  lazy="false"/>
        <set name="ORM_Producto" lazy="true" cascade="save-update,lock" inverse="true">
            <key column="Categoriasid" not-null="true"/>
            <one-to-many class="productos.Producto"/>
        </set>
    </class>
</hibernate-mapping>
