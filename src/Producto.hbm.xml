<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="productos.Producto" table="Producto" lazy="false">
        <id name="id" column="id" type="integer" unsaved-value="0">
            <generator class="native">
            </generator>
        </id>
        <property name="nombreProducto" column="nombreProducto" type="integer" length="10" not-null="false"
                  lazy="false"/>
        <many-to-one name="categorias" cascade="lock" column="Categoriasid" class="productos.Categoria" not-null="true"
                     lazy="proxy" access="field">
        </many-to-one>
        <set name="ORM_Item" lazy="true" cascade="save-update,lock" inverse="true">
            <key column="Productosid" not-null="true"/>
            <one-to-many class="productos.Item"/>
        </set>
    </class>
</hibernate-mapping>
